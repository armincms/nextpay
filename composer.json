{
    "name": "armincms/nextpay",
    "description": "An Armincms paymnet gateway.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "shetabit/payment": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\NextPay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\NextPay\\NextPayServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
